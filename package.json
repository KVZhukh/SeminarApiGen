{
    "name": "auchan-customer-gui-frontend-nextjs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev-https": "node run-ssl-proxy",
        "prod-https": "pnpm run build && NODE_ENV=production node run-ssl-proxy",
        "prod": "pnpm run build && pnpm run start",
        "prod:serve": "pnpm run start",
        "build": "next build",
        "build:analyze": "ANALYZE=true pnpm run build",
        "start": "next start",
        "test": "jest --silent",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint ./src --ext .js,.ts,.tsx,.mdx --fix",
        "lint:scss": "stylelint \"./src/**/*.scss\" --fix",
        "format": "prettier ./src --write --ignore-path .prettierignore",
        "tsc": "tsc",
        "tokens": "gds-tokens",
        "scss-tokens": "gds-scss-tokens",
        "typescript-gen": "ts-openapi-gen generate",
        "repack": "rm -rf ./node_modules && pnpm store prune && pnpm i",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky"
    },
    "dependencies": {
        "@babel/plugin-transform-private-property-in-object": "^7.25.9",
        "@greensight/gds": "^4.1.1",
        "@jest/globals": "^29.7.0",
        "@juggle/resize-observer": "^3.4.0",
        "@next/bundle-analyzer": "15.0.2",
        "@nimpl/getters": "^1.4.1",
        "@tanstack/react-query": "^5.59.19",
        "@tanstack/react-query-devtools": "^5.59.19",
        "classnames": "2.5.1",
        "cookie": "^1.0.1",
        "crypto-js": "^4.2.0",
        "csstype": "^3.1.3",
        "downshift": "9.0.8",
        "eslint-plugin-react-server-components": "^1.2.0",
        "fast-deep-equal": "3.1.3",
        "husky": "9.1.6",
        "lodash": "^4.17.21",
        "next": "^14.2.17",
        "next-compose-plugins": "^2.2.1",
        "next-plugin-svgr": "^1.1.10",
        "normalize.css": "^8.0.1",
        "popper-max-size-modifier": "^0.2.0",
        "react": "^18.3.1",
        "react-cookie": "7.2.2",
        "react-dom": "18.3.1",
        "react-dropzone": "^14.3.5",
        "react-fast-compare": "3.2.2",
        "react-merge-refs": "2.1.1",
        "react-transition-state": "^2.1.3",
        "schema-dts": "^1.1.2",
        "sharp": "^0.33.5",
        "ua-parser-js": "^1.0.39",
        "webpack": "^5.96.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/helper-plugin-utils": "^7.25.9",
        "@babel/preset-react": "^7.25.9",
        "@ensi-platform/typescript-openapi-generator": "2.0.0-beta.2",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/crypto-js": "^4.2.2",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.13",
        "@types/node": "22.9.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "18.3.1",
        "@types/ua-parser-js": "^0.7.39",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "babel-jest": "^29.7.0",
        "babel-plugin-react-require": "4.0.3",
        "css-loader": "7.1.2",
        "eslint": "^8.57.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "^14.2.17",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.9.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "15.2.10",
        "postcss-import": "^16.1.0",
        "prettier": "3.3.3",
        "sass": "^1.80.6",
        "sass-loader": "^16.0.5",
        "stylelint": "^16.10.0",
        "stylelint-config-prettier-scss": "^1.0.0",
        "stylelint-config-standard-scss": "^13.1.0",
        "stylelint-prettier": "^5.0.2",
        "ts-jest": "^29.2.5",
        "tslib": "^2.8.1",
        "typescript": "5.6.3",
        "typescript-plugin-css-modules": "^5.1.0"
    },
    "resolutions": {
        "cross-spawn": "^7.0.5"
    },
    "browserslist": [
        "last 2 versions",
        "not dead",
        "not IE 11"
    ],
    "lint-staged": {
        "*.{js,ts,tsx,mdx}": "eslint --cache --fix",
        "*.{scss}": "stylelint --cache --fix",
        "*.prettierignore": "prettier --write"
    }
}
